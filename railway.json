{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "env": {
      "NIXPACKS_LANGUAGE_OVERRIDE": "python",
      "NIXPACKS_PYTHON_VERSION": "3.11"
    }
  },
  "deploy": {
    "startCommand": "python backend/railway_start.py",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300
  }
}